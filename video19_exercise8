zariA1 = int(input("Give me a number: "))
zariA2 = int(input("Give me a number: "))
zariB1 = int(input("Give me a number: "))
zariB2 = int(input("Give me a number: "))
if zariA1+zariA2  > zariB1+zariB2 :
    print("Player A wins")
elif zariA1+zariA2  < zariB1+zariB2 :
    print("Player B wins")
else:
    print("Nobody wins")
